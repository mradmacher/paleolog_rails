<% content_for :navigation do %>
  <div id="navigation">
    <div class="navigation-section">
    <span class="navigation-section-title">Project</span>
    <ul>
      <% Project.viewable_by(current_user).each do |project| %>
        <li class="<%= project == @project ? 'current-item' : 'item' %>"><%= link_to project.name, project_path( project ) %></li>
      <% end %>
    </ul></div>
  </div>
<% end %>

